{
	"name": "@thatrichan/organizer-app",
	"tasks": {
		"dev": "deno run -A npm:vite",
		"build": "deno run -A npm:vite build",
		"preview": "deno run -A npm:vite preview",
		"check": "deno run -A npm:svelte-check"
	},
	"lint": {
		"exclude": [
			".vite",
			"dist",
			"./vite.config.*.timestamp-*.*"
		]
	},
	"fmt": {
		"exclude": [
			".vite",
			"dist",
			"./src/assets/css"
		]
	},
	"exports": {},
	"imports": {
		"@iconify/svelte": "npm:@iconify/svelte@^4.0.2",
		"@sveltejs/vite-plugin-svelte": "npm:@sveltejs/vite-plugin-svelte@^4.0.0",
		"autoprefixer": "npm:autoprefixer@^10.4.20",
		"postcss": "npm:postcss@^8.4.47",
		"postcss-load-config": "npm:postcss-load-config@^6.0.1",
		"svelte": "npm:svelte@^5.1.0",
		"svelte-check": "npm:svelte-check@^4.0.5",
		"svelte-i18n": "npm:svelte-i18n@^4.0.1",
		"svelte-preprocess": "npm:svelte-preprocess@^6.0.3",
		"svelte-spa-router": "npm:svelte-spa-router@^4.0.1",
		"tailwindcss": "npm:tailwindcss@^3.4.14",
		"vite": "npm:vite@^5.4.10"
	}
}
